{
  "singleQuote": true,
  "semi": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "printWidth": 80,
  "proseWrap": "always",
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "tailwindFunctions": ["clsx", "twMerge", "cn"],
  "plugins": ["prettier-plugin-tailwindcss"]
}